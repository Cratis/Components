{
    "name": "@cratis/components",
    "version": "1.0.0",
    "description": "",
    "author": "Cratis",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cratis/arc.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist",
        "**/*.ts",
        "**/*.tsx"
    ],
    "type": "module",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/esm/index.d.ts",
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        },
        "./commands": {
            "types": "./dist/esm/commands/index.d.ts",
            "require": "./dist/cjs/commands/index.js",
            "import": "./dist/esm/commands/index.js"
        },
        "./queries": {
            "types": "./dist/esm/queries/index.d.ts",
            "require": "./dist/cjs/queries/index.js",
            "import": "./dist/esm/queries/index.js"
        },
        "./dialogs": {
            "types": "./dist/esm/dialogs/index.js",
            "require": "./dist/cjs/dialogs/index.js",
            "import": "./dist/esm/dialogs/index.js"
        },
        "./identity": {
            "types": "./dist/esm/identity/index.d.ts",
            "require": "./dist/cjs/identity/index.js",
            "import": "./dist/esm/identity/index.js"
        }
    },
    "scripts": {
        "prepare": "yarn g:build",
        "clean": "yarn g:clean",
        "build": "yarn g:build",
        "lint": "yarn g:lint",
        "lint:ci": "yarn g:lint:ci",
        "test": "yarn g:test",
        "ci": "yarn g:ci",
        "up": "yarn g:up"
    },
    "dependencies": {
        "@cratis/arc": "18.5.0",
        "@cratis/arc.react": "18.5.0",
        "allotment": "1.20.4",
        "chart.js": "^4.5.1",
        "echarts": "6.0.0",
        "framer-motion": "12.23.26",
        "mobx": "6.15.0",
        "mobx-react": "9.2.1",
        "pixi.js": "^8.14.3",
        "primeicons": "7.0.0",
        "primereact": "10.9.7",
        "quill": "^2.0.3",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "react-icons": "5.5.0",
        "react-router-dom": "7.10.1",
        "reflect-metadata": "0.2.2",
        "ts-deepmerge": "7.0.3",
        "tsyringe": "4.10.0",
        "usehooks-ts": "3.1.1"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0"
    }
}
